{
	"manifest_version": 2,
	"name": "Fitbit Progress Indicator",
	"description": "The extension to help you get back on track with your step goal for the day.",
	"version": "2.1.0",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"permissions": [
		"http://www.fitbit.com/"
	],
	"content_scripts": [{
		"matches": ["http://www.fitbit.com/"],
		"js": ["jquery.js", "progress_indicator.js"]
	}]
}